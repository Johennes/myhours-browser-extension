{
  "manifest_version": 2,
  "name": "myhours",
  "description": "Track your time on myhours.com",
  "version": "0.1.2",
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png",
    "96": "assets/icon-96.png",
    "128": "assets/icon-128.png",
    "256": "assets/icon-256.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "assets/icon-16.png",
      "32": "assets/icon-32.png",
      "48": "assets/icon-48.png",
      "96": "assets/icon-96.png",
      "128": "assets/icon-128.png",
      "256": "assets/icon-256.png"
    },
    "default_title": "Track time on myhours",
    "default_popup": "src/static/Popup.html"
  },
  "options_ui": {
    "page": "src/static/Options.html"
  },
  "permissions": [
    "*://api2.myhours.com/api/*",
    "storage"
  ]
}
